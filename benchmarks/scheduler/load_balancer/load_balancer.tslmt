//#LIA#
always guarantee {
	enqueue && gt cpu0 cpu1 -> [cpu0 <- add cpu0 c1()];
	enqueue && gt cpu1 cpu0 -> [cpu1 <- add cpu1 c1()];

	(!(enqueue) && gt cpu0 cpu1) -> [cpu0 <- add cpu0 c1()] && [cpu1 <- sub cpu1 c0()];
	(!(enqueue) && gt cpu1 cpu0) -> [cpu1 <- add cpu1 c1()] && [cpu0 <- sub cpu0 c0()];

	!(G gt cpu0 cpu1);
	!(G gt cpu1 cpu0);
}
