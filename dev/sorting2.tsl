//#LIA#
always assume {
	(
		(!leq a b ) && 
		([a <- b] && [b <- a])
	) -> X (leq a b);

	(
		(!leq b c ) &&
		([b <- c] && [c <- b])
	) -> X (leq b c);

	(
		( leq a b ) &&
		([b <- c] && [c <- b])
	) -> X (leq a c);

	(
		( leq a c ) &&
		([a <- b] && [b <- a])
	) -> X (leq b c);


}

always guarantee {
	([a <- b] && [b <- a] && [c <- c]) ||
	([a <- a] && [b <- c] && [c <- b]) ||
	([a <- c] && [b <- b] && [c <- a]);
	F (leq a b && leq b c);
}
